{
	"name": "library-host",
	"version": "0.0.0",
	"scripts": {
		"ng": "ng",
		"start": "ng serve",
		"build:host": "ng build",
		"test:host": "ng test",
		"lint:host": "ng lint",
		"build": "ng build ngx-window-token --prod && npm run copy-readme",
		"test": "ng test ngx-window-token",
		"lint": "ng lint ngx-window-token",
		"publish:lib": "yarn publish ./dist/ngx-window-token",
		"publish:lib:next": "yarn publish ./dist/ngx-window-token --tag next",
		"copy-readme": "cpx \"./README.md\" \"./dist/ngx-window-token\"",
		"e2e": "ng e2e"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "~10.0.4",
		"@angular/common": "~10.0.4",
		"@angular/compiler": "~10.0.4",
		"@angular/core": "~10.0.4",
		"@angular/forms": "~10.0.4",
		"@angular/platform-browser": "~10.0.4",
		"@angular/platform-browser-dynamic": "~10.0.4",
		"@angular/router": "~10.0.4",
		"ngx-window-token": "4.0.0-beta01",
		"rxjs": "~6.6.0",
		"tslib": "^2.0.0",
		"zone.js": "~0.10.3"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "~0.1000.3",
		"@angular-devkit/build-ng-packagr": "~0.1000.3",
		"@angular/cli": "~10.0.3",
		"@angular/compiler-cli": "~10.0.4",
		"@angular/language-service": "~10.0.4",
		"@types/node": "^12.11.1",
		"@types/jasmine": "~3.5.0",
		"@types/jasminewd2": "~2.0.3",
		"codelyzer": "^6.0.0",
		"cpx": "^1.5.0",
		"jasmine-core": "~3.5.0",
		"jasmine-spec-reporter": "~5.0.0",
		"karma": "~5.0.0",
		"karma-chrome-launcher": "~3.1.0",
		"karma-coverage-istanbul-reporter": "~3.0.2",
		"karma-jasmine": "~3.3.0",
		"karma-jasmine-html-reporter": "^1.5.0",
		"ng-packagr": "^10.0.0",
		"protractor": "~7.0.0",
		"ts-node": "~8.3.0",
		"tslint-config-prettier": "1.18.0",
		"tslint": "~6.1.0",
		"typescript": "~3.9.7"
	}
}

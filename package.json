{
  "name": "library-host",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build:host": "ng build",
    "test:host": "ng test",
    "lint:host": "ng lint",
    "prettier": "prettier --write \"**/*.{json,scss,md,ts,html}\"",
    "build": "ng build ngx-window-token --configuration production && npm run copy-readme",
    "test": "ng test ngx-window-token",
    "lint": "ng lint ngx-window-token",
    "publish:lib": "yarn publish ./dist/ngx-window-token",
    "publish:lib:next": "yarn publish ./dist/ngx-window-token --tag next",
    "copy-readme": "cpx \"./README.md\" \"./dist/ngx-window-token\"",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.1.4",
    "@angular/common": "^15.1.4",
    "@angular/compiler": "^15.1.4",
    "@angular/core": "^15.1.4",
    "@angular/forms": "^15.1.4",
    "@angular/platform-browser": "^15.1.4",
    "@angular/platform-browser-dynamic": "^15.1.4",
    "@angular/router": "^15.1.4",
    "ngx-window-token": "6.0.0",
    "rxjs": "~7.4.0",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.1.5",
    "@angular/cli": "^15.1.5",
    "@angular/compiler-cli": "^15.1.4",
    "@angular/language-service": "^15.1.4",
    "@types/node": "^16.11.7",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "^6.0.0",
    "cpx": "^1.5.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^15.1.2",
    "protractor": "~7.0.0",
    "ts-node": "~10.4.0",
    "tslint-config-prettier": "1.18.0",
    "tslint": "~6.1.0",
    "typescript": "~4.8.4"
  }
}
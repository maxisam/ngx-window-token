{
  "name": "library-host",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build:host": "ng build",
    "test:host": "ng test",
    "lint:host": "ng lint",
    "prettier": "prettier --write \"**/*.{json,scss,md,ts,html}\"",
    "build": "ng build ngx-window-token --configuration production && npm run copy-readme",
    "test": "ng test ngx-window-token",
    "lint": "ng lint ngx-window-token",
    "publish:lib": "yarn publish ./dist/ngx-window-token",
    "publish:lib:next": "yarn publish ./dist/ngx-window-token --tag next",
    "copy-readme": "cpx \"./README.md\" \"./dist/ngx-window-token\"",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.13",
    "@angular/common": "~12.2.13",
    "@angular/compiler": "~12.2.13",
    "@angular/core": "~12.2.13",
    "@angular/forms": "~12.2.13",
    "@angular/platform-browser": "~12.2.13",
    "@angular/platform-browser-dynamic": "~12.2.13",
    "@angular/router": "~12.2.13",
    "ngx-window-token": "4.0.0-beta01",
    "rxjs": "~6.6.0",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.13",
    "@angular/cli": "~12.2.13",
    "@angular/compiler-cli": "~12.2.13",
    "@angular/language-service": "~12.2.13",
    "@types/node": "^12.11.1",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "^6.0.0",
    "cpx": "^1.5.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^12.2.5",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint-config-prettier": "1.18.0",
    "tslint": "~6.1.0",
    "typescript": "~4.3.5"
  }
}